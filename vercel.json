{
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "methods": [
        "POST",
        "GET"
      ],
      "dest": "api/webhook.js"
    },
    {
      "src": "/api/line-login",
      "methods": [
        "POST"
      ],
      "dest": "api/webhook.js"
    },
    {
      "src": "/api/messages",
      "dest": "api/messages.js"
    },
    {
      "src": "/api/markAsRead",
      "dest": "api/messages.js"
    },
    {
      "src": "/api/uploadImage",
      "dest": "api/messages.js"
    },
    {
      "src": "/api/orders",
      "methods": [
        "GET",
        "POST"
      ],
      "dest": "api/orders.js"
    },
    {
      "src": "/api/orders/(.*)",
      "methods": [
        "GET",
        "PUT",
        "DELETE"
      ],
      "dest": "api/orders.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    }
  ]
}